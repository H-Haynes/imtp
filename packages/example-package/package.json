{
  "name": "@imtp/example-package",
  "version": "1.0.0",
  "description": "An example package for IMTP monorepo",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "build:esm": "vite build --mode esm",
    "build:cjs": "vite build --mode cjs",
    "build:umd": "vite build --mode umd",
    "build:min": "node ../../scripts/build-min.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx --no-warn-ignored",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "example",
    "utility"
  ],
  "author": "IMTP",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@imtp/types": "workspace:*"
  }
}
